{
  "manifest_version": 2,
  "name": "YT Links Explorer",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "yt.png",
    "default_title": "YT Links Explorer",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "yt.png"
  },
  "options_page": "video_player.html"
}